{{#ifwith resume.references}}
	<section>
		{{> section-header name="References"}}
		<ul id="references-list">
			{{#each this}}
				<li>
					<header>
						<h3>{{name}}</h3>
					</header>
					{{#if reference}}
						<p>{{reference}}</p>
					{{/if}}
				</li>
			{{/each}}
		</ul>
	</section>
{{/ifwith}}