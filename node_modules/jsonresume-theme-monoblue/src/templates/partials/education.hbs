{{#ifwith resume.education}}
	<section>
		{{> section-header name="Education"}}
		<ul id="education-list">
			{{#each this}}
				<li>
					<header>
						<div class="flex-between">
							<h3>
								{{#if studyType}}
									{{studyType}}
									{{#if area}}
										<span>of {{area}}</span>
									{{/if}}
								{{else}}
									{{area}}
								{{/if}}
							</h3>
							<h5>
								{{startDate}}
								{{#if endDate}}
									<span>- {{endDate}}</span>
								{{else}}
									<span>- Present</span>
								{{/if}}
							</h5>
						</div>
						{{#if institution}}
							<h4>
								{{institution}}
								{{#if url}}
									<a target="_blank" href="{{url}}">
										<span class="fas fa-fw fa-external-link-alt"></span>
									</a>
								{{/if}}
							</h4>
						{{else if url}}
							<h4><a target="_blank" href="{{url}}">{{url}}</a></h4>
						{{/if}}
					</header>
					{{#if score}}
						<strong>Grade:</strong> {{score}}
					{{/if}}
					{{#ifwith courses}}
						<ul>
							{{#each this}}
								<li>{{.}}</li>
							{{/each}}
						</ul>
					{{/ifwith}}
				</li>
			{{/each}}
		</ul>
	</section>
{{/ifwith}}