{{#ifwith resume.volunteer}}
	<section>
		{{> section-header name="Volunteer"}}
		<ul id="volunteer-list">
			{{#each this}}
				<li>
					<header>
						<div class="flex-between">
							<h3>
								{{#if organization}}
									{{organization}}
									{{#if url}}
										<a target="_blank" href="{{url}}">
											<span class="fas fa-fw fa-external-link-alt"></span>
										</a>
									{{/if}}
								{{else if url}}
									<a target="_blank" href="{{url}}">{{url}}</a>
								{{/if}}
							</h3>
							<h5>
								{{startDate}}
								{{#if endDate}}
									<span>- {{endDate}}</span>
								{{else}}
									<span>- Present</span>
								{{/if}}
							</h5>
						</div>
						{{#if position}}
							<h4>{{position}}</h4>
						{{/if}}
					</header>
					{{#if summary}}
						<div class="summary">
							<p>{{summary}}</p>
						</div>
					{{/if}}
					{{#if highlights.length}}
						<ul class="highlights">
							{{#each highlights}}
								<li>{{.}}</li>
							{{/each}}
						</ul>
					{{/if}}
				</li>
			{{/each}}
		</ul>
	</section>
{{/ifwith}}