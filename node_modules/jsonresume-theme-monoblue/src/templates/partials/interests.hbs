{{#ifwith resume.interests}}
	<section>
		{{> section-header name="Interests"}}
		<ul id="interests-list">
			{{#each this}}
				<li>
					<header>
						<h3>{{name}}</h3>
					</header>
					{{#ifwith keywords}}
						<ul>
							{{#each this}}
								<li>{{.}}</li>
							{{/each}}
						</ul>
					{{/ifwith}}
				</li>
			{{/each}}
		</ul>
	</section>
{{/ifwith}}