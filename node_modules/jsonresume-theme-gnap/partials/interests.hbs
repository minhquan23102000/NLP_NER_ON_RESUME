{{#if resume.interests}}
<section id="interests" class="valign-wrapper">
  <div class="container">
    {{#if resume.meta.headers.interests}}<h2>{{resume.meta.headers.interests}}</h2>{{/if}}
    <ul>
      {{#each resume.interests}}
      <li>{{name}}</li>
      {{/each}}
    </ul>
  </div>
</section>
{{/if}}