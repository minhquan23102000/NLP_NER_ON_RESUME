<div class="menu" role="navigation" aria-label="primary">
  <div>
    <ul>
      {{#if resume.meta.contactForm}}{{#if resume.meta.contactForm.header}}{{#if resume.meta.contactForm.action}}{{#if resume.meta.contactForm.fields}}
      <li>
        <a href="#contact" title="{{#if resume.meta.contactForm.header}}{{resume.meta.contactForm.header}}{{else}}Contact Form{{/if}}"><i class="fas fa-envelope"></i></a>
      </li>{{/if}}{{/if}}{{/if}}
      {{else}}{{#if resume.basics.email}}
      <li><a href="mailto:{{resume.basics.email}}"><i class="fas fa-envelope"></i></a></li>
      {{/if}}{{/if}}
      {{#resume.basics}}
        {{#if profiles}}{{#each profiles}}
          <li><a href="{{url}}" title="{{network}}"><i class="fab fa-{{toLowerCase network}}"></i></a></li>
        {{/each}}{{/if}}
      {{/resume.basics}}
    </ul>
  </div>
</div>