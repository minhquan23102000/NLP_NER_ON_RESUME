<footer class="grey darken-4 white-text">
  <div class="container">
    {{> contact-form}}
    {{#resume.basics}}
    {{#if profiles}}
      <ul class="row center inline">
        {{#each profiles}}
          <li><a href="{{url}}" title="{{network}}"><i class="fab fa-{{toLowerCase network}}"></i></a></li>
        {{/each}}
      </ul>
    {{/if}}
    {{/resume.basics}}
    <ul class="row center inline inline-delimited">
      {{#if resume.basics.summary}}{{#if resume.meta.headers.basics}}<li><a href="#basics">{{resume.meta.headers.basics}}</a></li>{{/if}}{{/if}}
      {{#if resume.skills}}{{#if resume.meta.headers.skills}}<li><a href="#skills">{{resume.meta.headers.skills}}</a></li>{{/if}}{{/if}}
      {{#if resume.languages}}{{#if resume.meta.headers.languages}}<li><a href="#languages">{{resume.meta.headers.languages}}</a></li>{{/if}}{{/if}}
      {{#if resume.projects}}{{#if resume.meta.headers.projects}}<li><a href="#projects">{{resume.meta.headers.projects}}</a></li>{{/if}}{{/if}}
      {{#if resume.work}}{{#if resume.meta.headers.work}}<li><a href="#work">{{resume.meta.headers.work}}</a></li>{{/if}}{{/if}}
      {{#if resume.volunteer}}{{#if resume.meta.headers.volunteer}}<li><a href="#volunteer">{{resume.meta.headers.volunteer}}</a></li>{{/if}}{{/if}}
      {{#if resume.education}}{{#if resume.meta.headers.education}}<li><a href="#education">{{resume.meta.headers.education}}</a></li>{{/if}}{{/if}}
      {{#if resume.certificates}}{{#if resume.meta.headers.certificates}}<li><a href="#certificates">{{resume.meta.headers.certificates}}</a></li>{{/if}}{{/if}}
      {{#if resume.awards}}{{#if resume.meta.headers.awards}}<li><a href="#awards">{{resume.meta.headers.awards}}</a></li>{{/if}}{{/if}}
      {{#if resume.publications}}{{#if resume.meta.headers.publications}}<li><a href="#publications">{{resume.meta.headers.publications}}</a></li>{{/if}}{{/if}}
      {{#if resume.interests}}{{#if resume.meta.headers.interests}}<li><a href="#interests">{{resume.meta.headers.interests}}</a></li>{{/if}}{{/if}}
      {{#if resume.references}}{{#if resume.meta.headers.references}}<li><a href="#references">{{resume.meta.headers.references}}</a></li>{{/if}}{{/if}}
    </ul>
    <div class="row center">
      &copy; {{year}} {{resume.basics.name}}
    </div>
  </div>
</footer>